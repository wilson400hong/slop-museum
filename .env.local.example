# Mock mode: set to 'true' for local dev without Supabase
# When true, all data is stored in-memory and resets on restart
# Set to 'false' (or remove) to use real Supabase
NEXT_PUBLIC_USE_MOCK=true

# Supabase - Only needed when NEXT_PUBLIC_USE_MOCK is NOT 'true'
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
